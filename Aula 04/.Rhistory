, data = dados)
##---Apresentação dos resultados;
summary(mod1)
#---gráfico dos resíduos...;
par(mfrow=c(2,2))
plot(mod1)
X1 <- mean(dados$esperanca_vida_ao_nascer)
X2 <- mean(dados$IDHM_educacao)
y_hat <- mod1$coefficients[1] + mod1$coefficients[2]*X1 +  mod1$coefficients[3]*X2
y_hat2 <- y_hat + mod1$coefficients[3]
x2_hat <- (y_hat2 -  mod1$coefficients[1] - mod1$coefficients[2]*X1)/ mod1$coefficients[3]
var_x2 <- x2_hat - X2
